    "version": "0.2.0",
    "configurations": [
        {
            "name": "Ingestion: Mobilisights (Debug)",
            "type": "debugpy",
            "request": "launch",
            "program": "src/ingestion/mobilisights/__main__.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Compressor: Stellantis (Debug)",
            "type": "debugpy",
            "request": "launch",
            "module": "src.transform.compressor.main",
            "args": [
                "stellantis"
            ]
        },
        {
            "name": "FastAPI: WebIngestion (Debug)",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "web.app.main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "FastAPI: External API (Debug)",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "src.external_api.app:app",
                "--reload"
            ],
            "jinja": true
        }
    ]
}

